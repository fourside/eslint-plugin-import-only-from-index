{
  "name": "eslint-plugin-import-only-from-index",
  "version": "1.0.0",
  "description": "Eslint plugin to enforce import only from index file",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "prebuild": "npm run clean",
    "clean": "node -e 'fs.rmSync(`dist`, {recursive: true, force: true})'",
    "test": "vitest --dir src --coverage",
    "lint": "eslint --ext .ts src",
    "check": "tsc --noEmit && npm run lint && npm run test -- --run",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/eslint": "^8.4.1",
    "@types/estree": "^0.0.51",
    "@types/node": "^17.0.22",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "c8": "^7.11.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-eslint-plugin": "^4.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "prettier": "^2.6.0",
    "typescript": "^4.6.2",
    "vitest": "^0.7.7"
  },
  "keywords": [],
  "author": "fourside@gmail.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/fourside/eslint-plugin-import-only-from-index"
  },
  "license": "MIT",
  "files": [
    "lib",
    "!lib/*.test.js"
  ],
  "prettier": {
    "singleQuote": false,
    "printWidth": 120,
    "tabWidth": 2,
    "trailingComma": "es5"
  }
}
